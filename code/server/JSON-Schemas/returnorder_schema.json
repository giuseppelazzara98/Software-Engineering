      "returnDate": {
        "type": "string"
      },
      "aisleID": {
        "type": "string",
        "maxLength": 4,
        "minLength": 4
      },
      "row": {
        "type": "string",
        "maxLength": 4,
        "minLength": 4
      },
      "col": {
        "type": "string",
        "maxLength": 4,
        "minLength": 4
      },
      "maxWeight": {
        "type": "integer"
     },
      "maxVolume": {
        "type": "integer"
      }
    },
    "required": ["returnorderID", "aisleID", "row", "col", "maxWeight", "maxVolume"]
  }